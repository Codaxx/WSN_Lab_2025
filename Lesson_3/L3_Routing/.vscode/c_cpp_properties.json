        "${workspaceFolder}/**",
        "${HOME}/contiki-ng/",
        "${HOME}/contiki-ng/os/",
        "${HOME}/contiki-ng/os/sys/",
        "${HOME}/contiki-ng/arch/cpu/",
        "${HOME}/contiki-ng/arch/cpu/nrf52840/",
